{
    "ecmaFeatures":{
        "jsx": true
    },
    "env":{
        "browser":true,
        "node": true,
        "jquery": true
    },
    "rules": {
        "quotes": "off",
        "no-trailing-spaces": "off",
        "eol-last": "off",
        "no-unused-vars": "error",
        "no-underscore-dangle": "off",
        "no-alert": "error",
        "no-lone-blocks": "off"
    },
    "globals":{
        "jQuery": true,
        "$": true
    }
}